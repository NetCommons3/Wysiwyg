    "authors": [
        {
            "name": "NetCommons Community",
            "homepage": "http://www.netcommons.org"
        }
    ],
    "type":        "cakephp-plugin",
    "keywords":    ["cakephp", "wysiwyg"],
    "config": {
        "vendor-dir": "vendors"
    }
}

